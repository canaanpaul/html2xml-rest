<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <display-name>html2xml REST API</display-name>
  <description>html2xml REST API</description>

  <servlet>
    <servlet-name>html2xml</servlet-name>
    <servlet-class>org.trancecode.web.html2xml.Html2XmlServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>html2xml</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>

</web-app>
